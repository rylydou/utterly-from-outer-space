config_version=5

[application]

config/name="GMTK23"
run/main_scene="res://blakeplayground.tscn"
config/features=PackedStringArray("4.1", "Forward Plus")
config/icon="res://icon.svg"

[autoload]

Hud="*res://scenes/hud.tscn"
SoundBank="*res://addons/soundbank/soundbank.gd"

[debug]

gdscript/warnings/enable=false
gdscript/warnings/unused_variable=0
gdscript/warnings/unused_local_constant=0
gdscript/warnings/unused_private_class_variable=0
gdscript/warnings/unused_parameter=0
gdscript/warnings/unused_signal=0

[display]

window/size/viewport_width=1280
window/size/viewport_height=720
window/stretch/mode="canvas_items"
window/stretch/aspect="keep_height"

[editor_plugins]

enabled=PackedStringArray("res://addons/goshapes/plugin.cfg", "res://addons/soundbank/plugin.cfg")

